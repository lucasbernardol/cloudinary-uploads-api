    "@types/node": "^17.0.21",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5"
  }
