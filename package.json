    "typeorm:cli": "ts-node-dev --transpile-only ./node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "cloudinary": "^1.28.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
